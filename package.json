{
  "name": "wwe-access-organization",
  "version": "1.0.0",
  "description": "WWE Access Organization - Your ultimate source for WWE championship information and superstar profiles",
  "main": "backend/server.js",
  "scripts": {
    "start": "node backend/server.js",
    "server": "nodemon backend/server.js",
    "client": "serve -s src",
    "dev": "concurrently &quot;npm run server&quot; &quot;npm run client&quot;",
    "seed": "node backend/seed/seed.js",
    "build": "node src/tools/deploy.js",
    "test": "jest",
    "create-profile": "node src/tools/create-superstar-profile.js",
    "update-champion": "node src/tools/update-champion.js"
  },
  "keywords": [
    "WWE",
    "wrestling",
    "championships",
    "superstars",
    "profiles"
  ],
  "author": "WWE Access Organization",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "mongoose": "^7.5.0",
    "serve": "^14.2.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.1",
    "jest": "^29.6.4",
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}