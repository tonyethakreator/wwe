    <script src="../assets/js/main.js"></script>
    <script>
        document.addEventListener('DOMContentLoaded', function() {
            // Tab functionality
            const tabs = document.querySelectorAll('.championship-tab');
            const contents = document.querySelectorAll('.championship-content');
            
            tabs.forEach(tab => {
                tab.addEventListener('click', () => {
                    const target = tab.getAttribute('data-target');
                    
                    // Remove active class from all tabs and contents
                    tabs.forEach(t => t.classList.remove('active'));
                    contents.forEach(c => c.classList.remove('active'));
                    
                    // Add active class to clicked tab and corresponding content
                    tab.classList.add('active');
                    document.querySelector(`.championship-content[data-id="${target}"]`).classList.add('active');
                });
            });
            
            // Handle hash links
            if (window.location.hash) {
                const hash = window.location.hash.substring(1);
                const tabToActivate = document.querySelector(`.championship-tab[data-target="${hash}"]`);
                
                if (tabToActivate) {
                    tabToActivate.click();
                    document.querySelector('.championship-tabs').scrollIntoView({ behavior: 'smooth' });
                } else {
                    const championshipElement = document.getElementById(hash);
                    if (championshipElement) {
                        const parentContent = championshipElement.closest('.championship-content');
                        const tabId = parentContent.getAttribute('data-id');
                        document.querySelector(`.championship-tab[data-target="${tabId}"]`).click();
                        setTimeout(() => {
                            championshipElement.scrollIntoView({ behavior: 'smooth' });
                        }, 100);
                    }
                }
            }
            
            // Update URL hash when clicking on championship details
            const championshipDetails = document.querySelectorAll('.championship-detail');
            championshipDetails.forEach(detail => {
                detail.addEventListener('click', (e) => {
                    if (e.target.tagName !== 'A') {
                        const id = detail.getAttribute('id');
                        window.location.hash = id;
                    }
                });
            });
        });
    </script>
